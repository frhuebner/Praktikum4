@article{tlenergy,
  author={J C Knight and T H Braid and H O W Richardson},
  title={The Beta Rays of Thallium 204},
  journal={Proceedings of the Physical Society. Section A},
  volume={67},
  number={10},
  pages={881},
  url={http://stacks.iop.org/0370-1298/67/i=10/a=304},
  year={1954},
  abstract={The β-spectrum of 204 T1 has been measured using thin evaporated sources in a spheroidal field β-spectrometer. The end-point was found to be at 766±2 keV. Small deviations were found from the unique shape of spectrum predicted for a first-forbidden transition with a spin change of two units. The deviations extend up to about 400 keV. The possibility that the decay is second forbidden and that the ground state of 204 T1 has even parity is noted.}
}
                                          


